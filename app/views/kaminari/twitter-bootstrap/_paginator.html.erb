<%= paginator.render do %>
<div class="pagination">
<ul>
  <%= prev_page_tag -%>
  <% each_page do |page| %>
  <% if page.left_outer? or page.right_outer? or page.inside_window? %>
    <%= page_tag page %>
  <% elsif !page.was_truncated? %>
    <%= gap_tag %>
  <% end %>
  <% end %>
  <%= next_page_tag %>
</ul>
</div>
<% end %>
